  "files": [
    "dist",
    "index.js",
    "index.module.js"
  ],
  "keywords": [
    "form",
    "forms",
    "react",
    "spooner",
    "validate",
    "validation"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "name": "spooner",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0"
  },
  "repository": "adelarosab/spooner",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:ci": "tsc && npm run build",
    "build:cjs": "esbuild ./src/index.ts --bundle --format=cjs --outfile=./dist/index.cjs.js",
    "build:esm": "esbuild ./src/index.ts --bundle --format=esm --outfile=./dist/index.esm.js",
    "format": "npm run format:ci -- --write",
    "format:ci": "prettier --ignore-path .gitignore -l \"**/*.+(js|json|md|yml)\"",
    "lint": "eslint \"src/**/*.js\" --fix",
    "lint:ci": "npm run lint -- --max-warnings 0",
    "prepare": "npm run build && husky install",
    "release": "semantic-release",
    "start": "npm run test:watch",
    "test": "jest --silent",
    "test:ci": "npm run test -- --coverage",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "version": "0.0.0-development"
}
